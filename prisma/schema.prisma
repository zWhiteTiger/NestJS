generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model auth {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  password   String
  email      String   @unique
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
